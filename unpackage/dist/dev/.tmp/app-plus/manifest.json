{
  "@platforms": [
    "android",
    "iPhone",
    "iPad"
  ],
  "id": "__UNI__30911E9",
  "name": "uni-bluetooth",
  "version": {
    "name": "1.0.0",
    "code": "100"
  },
  "description": "蓝牙测试demo",
  "launch_path": "__uniapptabbar.html",
  "developer": {
    "name": "",
    "email": "",
    "url": ""
  },
  "permissions": {
    "OAuth": {},
    "Bluetooth": {},
    "Statistic": {},
    "Speech": {},
    "iBeacon": {},
    "UniNView": {
      "description": "UniNView原生渲染"
    }
  },
  "plus": {
    "useragent": {
      "value": "uni-app appservice",
      "concatenate": true
