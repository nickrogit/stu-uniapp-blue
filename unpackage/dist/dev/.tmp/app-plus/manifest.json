          "url": "https://ext.dcloud.net.cn/plugin?id=36",
          "android_package_name": "io.dcloud.UNI30911E9",
          "ios_bundle_id": "",
          "isCloud": true,
          "bought": 1,
          "pid": "36",
          "parameters": {}
        }
      }
    },
    "allowsInlineMediaPlayback": true,
    "safearea": {
      "background": "#F8F8F8",
      "bottom": {
        "offset": "auto"
      }
